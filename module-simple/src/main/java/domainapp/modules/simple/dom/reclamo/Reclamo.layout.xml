<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bs3:grid
        xsi:schemaLocation="https://causeway.apache.org/applib/layout/component https://causeway.apache.org/applib/layout/component/component.xsd https://causeway.apache.org/applib/layout/grid/bootstrap3 https://causeway.apache.org/applib/layout/grid/bootstrap3/bootstrap3.xsd"
        xmlns:cpt="https://causeway.apache.org/applib/layout/component"
        xmlns:bs3="https://causeway.apache.org/applib/layout/grid/bootstrap3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bs3:row>
        <bs3:col span="12" unreferencedActions="true">
            <cpt:domainObject bookmarking="AS_ROOT"/>
        </bs3:col>
    </bs3:row>
    <bs3:row>
        <bs3:col span="6">
            <bs3:row>
                <bs3:col span="12">
                    <cpt:fieldSet name="Datos del Reclamo">
                        <cpt:property  id="nroReclamo" namedEscaped="true"/>
                        <cpt:property  id="estado" namedEscaped="true"/>
                        <cpt:property  id="usuario" namedEscaped="true"/>
                        <cpt:property  id="direccion" namedEscaped="true"/>
                        <cpt:property  id="fecha" namedEscaped="true"/>
                        <cpt:property  id="tipoReclamo" namedEscaped="true"/>
                        <cpt:property  id="descripcion" namedEscaped="true" multiLine="8" hidden="ALL_TABLES"/>
                        <cpt:property  id="fechaCierre" namedEscaped="true"/>
                       <cpt:property  id="observacion" namedEscaped="true" multiLine="8" hidden="ALL_TABLES"/>
                    </cpt:fieldSet>
                </bs3:col>
            </bs3:row>
        </bs3:col>
    </bs3:row>
    <bs3:row>
        <bs3:col span="6">
            <bs3:row>
                <bs3:col span="6">
                    <bs3:tabGroup>
                        <bs3:tab name="Identity">
                            <bs3:row>
                                <bs3:col span="12">
                                    <cpt:fieldSet name="Other" id="other" unreferencedProperties="true"/>
                                </bs3:col>
                            </bs3:row>
                        </bs3:tab>
                        <bs3:tab name="Metadata">
                            <bs3:row>
                                <bs3:col span="12">
                                    <cpt:fieldSet name="Metadata" id="metadata"/>
                                </bs3:col>
                            </bs3:row>
                        </bs3:tab>
                    </bs3:tabGroup>
                </bs3:col>
            </bs3:row>
                <bs3:tabGroup  unreferencedCollections="true">
            </bs3:tabGroup>
        </bs3:col>
    </bs3:row>
</bs3:grid>
